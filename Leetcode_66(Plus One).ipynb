      "source": [
        "class Solution:\n",
        "    def plusOne(self, digits):\n",
        "      for i in range(len(digits)-1, -1, -1):\n",
        "         if digits[i]<9:\n",
        "          digits[i]+=1\n",
        "          break\n",
        "          digits[i]=0\n",
        "         else:\n",
        "          digits=[1] + digits\n",
        "      return digits"
      ]
    }
  ]
}
