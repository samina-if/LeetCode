        "def removeDuplicates(nums):\n",
        "  if not nums:\n",
        "        return 0\n",
        "  l=0\n",
        "  for r in range(1,len(nums)):\n",
        "        if nums[l]!=nums[r]:\n",
        "            l+=1\n",
        "            nums[l]=nums[r]\n",
        "\n",
        "  return l+1\n",
        "\n",
        "nums = [0,0,1,1,1,2,2,3,3,4]\n",
        "print(removeDuplicates(nums))\n"
      ]
    }
  ]
